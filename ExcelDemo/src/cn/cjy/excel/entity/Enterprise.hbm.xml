<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.cjy.excel.entity.Enterprise" table="zhongyuan">
         <id name="id" type="java.lang.Integer" column="id">
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String" >
       	<column name="name" length="45"></column>
        </property>
        <property name="dn" type="java.lang.Double" >
        <column name="dn"  not-null="true"></column>
        </property>
        <property name="ym" type="java.lang.Double" >
        <column name="ym"  not-null="true"></column>
        </property>
        <property name="cy" type="java.lang.Double" >
        <column name="cy"  not-null="true"></column>
        </property>
        <property name="tr" type="java.lang.Double" >
        <column name="tr"  not-null="true"></column>
        </property>
        <property name="zq" type="java.lang.Double" >
        <column name="zq"  not-null="true"></column>
        </property>
        <property name="date" type="java.lang.String" >
        <column name="date"  length="30"></column>
        </property>
    </class>
</hibernate-mapping>

